{
  "name": "ember-pikaday-shim",
  "version": "0.1.3",
  "description": "pikaday for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm run lint-js && npm run lint-md",
    "lint-js": "eslint *.js addon app blueprints config tests",
    "lint-md": "remark *.md",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-pikaday-shim.git"
  },
  "homepage": "https://github.com/ciena-blueplanet/ember-pikaday-shim",
  "engines": {
    "node": ">= 6.0.0"
  },
  "bugs": {
    "url": "https://github.com/ciena-blueplanet/ember-pikaday-shim/issues"
  },
  "author": "Matthew Dahl (https://github.com/sandersky)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "2.12.3",
    "ember-cli-chai": "^0.3.2",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.6",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "~0.13.2",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-source": "~2.12.0",
    "eslint": "^3.13.1",
    "eslint-config-frost-standard": "^5.3.2",
    "loader.js": "^4.2.3",
    "remark-cli": "^2.1.0",
    "remark-lint": "^5.4.0"
  },
  "keywords": [
    "ember-addon",
    "pikaday",
    "date"
  ],
  "dependencies": {
    "broccoli-funnel": "^1.0.4",
    "broccoli-merge-trees": "^1.1.4",
    "broccoli-metal": "^1.0.0",
    "ember-cli-babel": "^5.1.7",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-moment-shim": "^3.0.1",
    "pikaday": "1.6.1"
  },
  "ember-addon": {
    "after": [
      "ember-pikaday"
    ],
    "configPath": "tests/dummy/config"
  }
}
